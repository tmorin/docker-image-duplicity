MAIL_TO=""
SHELL=/bin/bash
BASH_ENV=/container.env
17 * * * * root cd /workdir && run-parts --report /etc/cron.hourly >/proc/1/fd/1 2>/proc/1/fd/2
25 6 * * * root cd /workdir && run-parts --report /etc/cron.daily >/proc/1/fd/1 2>/proc/1/fd/2
47 6 * * 7 root cd /workdir && run-parts --report /etc/cron.weekly >/proc/1/fd/1 2>/proc/1/fd/2
52 6 1 * * root cd /workdir && run-parts --report /etc/cron.monthly >/proc/1/fd/1 2>/proc/1/fd/2
